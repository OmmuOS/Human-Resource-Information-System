{
	"name": "ommu/1st-source",
	"description": "Ommu Platform First Source",
	"keywords": [
		"ommu",
		"platform",
		"ommu platform",
		"module",
		"users",
		"report",
		"support",
		"backoffice-theme",
		"cms"
	],
	"homepage": "https://github.com/PutraSudaryanto/OmmuFirstSource",
	"type": "project",
	"license": "Apache-2.0",
	"support": {
		"issues": "https://github.com/PutraSudaryanto/OmmuFirstSource/issues",
		"wiki": "https://github.com/PutraSudaryanto/OmmuFirstSource/wiki",
		"source": "https://github.com/PutraSudaryanto/OmmuFirstSource"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PutraSudaryanto/OmmuFirstSource.git"
	},
	"authors": [
		{
			"name": "Putra Sudaryanto",
			"email": "putra@sudaryanto.id",
			"homepage": "https://putra.sudaryanto.id",
			"role": "Creator"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"oomphinc/composer-installers-extender": "^1.0",
		"yiisoft/yii": "^1.1.14",
		"ommu/gii": "dev-master",
		"ommu/ommu": "dev-master",
		"yiiext/imperavi-redactor-widget": "^1.3.10",
		"mintao/yii-behavior-sluggable": "^1.0",
		"ommu/gapi-google-analytics-php-interface": "^1.0",
		"ommu/users": "dev-master",
		"ommu/support": "dev-master",
		"ommu/report": "dev-master"
	},
	"require-dev": {
	},
	"config": {
		"process-timeout": 1800,
		"preferred-install": "dist",
		"vendor-dir": "protected/vendor"
	},
	"extra": {
		"process-timeout": 1800,
		"installer-types": [
			"ommu-module",
			"ommu-library",
			"ommu-theme",
			"ommu-extension"
		],
		"installer-paths": {
			"protected/libraries/{$name}/": [
				"type:ommu-library"
			],
			"themes/{$name}/": [
				"type:ommu-theme"
			]
		}
	}
}